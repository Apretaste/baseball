<div class="row">
	<div class="col s12 l12 m12 x12">
		<h1><%= titulo %></h1>
	</div>
</div>

<div class="row">
	<div class="col s12 l12 m12 x12">
		<table style="text-align:center" width="100%">
	<tr>
		<th><div class="row">
	<div class="col s12 l12 m12 x12">
		<h2>Fecha</h2>	</div>
</div></th>
		<th><div class="row">
	<div class="col s12 l12 m12 x12">
		<h2>Local</h2>	</div>
</div></th>
		<th></th>
		<th><div class="row">
	<div class="col s12 l12 m12 x12">
		<h2>Visitante</h2>	</div>
</div></th>
		<th colspan="3"><div class="row">
	<div class="col s12 l12 m12 x12">
		<h2>Resultado</h2>	</div>
</div></th>
	</tr>
	<% for(var i=0; i < fixture.length; i++) {var juego = fixture[i]; %>

		<tr>
			<td><%= formatDate(juego.date) %></td>
			<td><%= juego.homeTeamName %></td>
			<td>-</td>
			<td><%= juego.awayTeamName %></td>
			<td><b><%= juego.result.goalsHomeTeam %></b></td>
			<td><b>:</b></td>
			<td><b><%= juego.result.goalsAwayTeam %></b></td>
		</tr>

	<% } %>
		</table>
	</div>
</div>



<div class="row">
	<div class="col s12 l12 m12 x12">
		<table style="text-align:center;" width="100%">
	<tr>
		<td style="" colspan="1">
			<a class="btn" href="#!" onclick="apretaste.send({command: 'FUTBOL LIGA', data: {query: '<%= liga.payload.id %>'}});">Ver Liga</a>
		</td>
	</tr>
	</table>
	</div>
</div>

