<div class="row">
  <div class="col s12 l12 m12 x12">
    <h1>Ultimas Noticias de Baseball en Cuba</h1>
  </div>
</div>
<small>Por el Neno Diaz</small>
<div class="row">
  <div class="col s12 l12 m12 x12">
    <table>
        <% for(var i = 0; i < noticias.length; i++) {
            var noticia = noticias[i];
        %>
        <tr>
          <td>
            <b>
              <a href="#!" onclick="apretaste.send({command: 'DIARIODECUBA HISTORIA', data:{query: '<%= noticia.link %>'}});">
                <%= noticia.titulo %>
              </a>
            </b>
          </td>
        </tr>
        <tr>
          <td>
            <%= noticia.descripcion.substr(0, 200) %>...
          </td>
        </tr>
        <% } %>
    </table>
  </div>
</div>