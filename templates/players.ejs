<div class="row">
    <div class="col">
        <h1>Juegadores de <%= team.name %></h1>
    </div>
</div>

<div class="row">
    <div class="col l12 m12 s12">
        <table class="striped">
            <% for(i = 0; i < players.length;i++) { var player = players[i]; %>
            <tr>
                <td>#<%= player.number %></td>
                <td><%= player.name %></td>
                <td><%= player.position %></td>
            </tr>
           <% } %>
        </table>
    </div>
</div>

<div class="fixed-action-btn">
    <a class="btn-floating btn-large waves-effect waves-light green" href="#!" onclick="apretaste.send({command: 'BASEBALL', data: {query: ''}});">
        <i class="material-icons">arrow_back</i>
    </a>
</div>
